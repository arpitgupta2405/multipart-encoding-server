# Server Configuration
PORT=3002

# Remote Upload Configuration
REMOTE_UPLOAD_ENABLED=false

# S3 Configuration
S3_UPLOAD_ENABLED=false
S3_BUCKET=your-s3-bucket-name
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=your-s3-access-key
S3_SECRET_ACCESS_KEY=your-s3-secret-key

# FTP Configuration
FTP_UPLOAD_ENABLED=false
FTP_HOST=your-ftp-host.com
FTP_PORT=21
FTP_USERNAME=your-ftp-username
FTP_PASSWORD=your-ftp-password
FTP_PATH=/uploads

# HTTP Upload Configuration
HTTP_UPLOAD_ENABLED=false
HTTP_UPLOAD_URL=https://your-api-endpoint.com/upload
HTTP_UPLOAD_METHOD=POST
HTTP_UPLOAD_HEADERS={"Authorization": "Bearer your-api-token", "Content-Type": "multipart/form-data"} 